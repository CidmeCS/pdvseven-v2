<UserControl x:Class="a7D.PDV.AutoAtendimento.UI.Controles.TecladoAlfanumerico"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:a7D.PDV.AutoAtendimento.UI.Controles"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Loaded="UserControl_Loaded">
    <Canvas Name="teclas">
        <Button Name="Backspace" Click="Btn_Click"></Button>
        <Button Name="NBackspace" Click="Btn_Click"></Button>
        <Button Name="Space" Click="Btn_Click"></Button>
    </Canvas>
</UserControl>
