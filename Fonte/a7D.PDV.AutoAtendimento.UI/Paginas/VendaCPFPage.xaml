<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:Controls="clr-namespace:a7D.PDV.AutoAtendimento.UI.Controles"
      x:Class="a7D.PDV.AutoAtendimento.UI.Paginas.VendaCPFPage"
      xmlns:Controles="clr-namespace:a7D.PDV.AutoAtendimento.UI.Controles"
      mc:Ignorable="d" 
      Loaded="Page_Loaded"
      d:DesignHeight="600" d:DesignWidth="800">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\Controles\StyleButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <Canvas>
        <Button Name="Home" Click="Cancelar_Click"/>
        <Button Name="SemCPF" Click="Confirmar_Click"/>
        <TextBlock Name="Titulo" />
        <Controles:TecladoBox x:Name="Teclado" Height="400" Width="300" onChange="Teclado_onChange" />
        <Button Name="Cancelar" Click="Cancelar_Click"/>
    </Canvas>
</Page>
