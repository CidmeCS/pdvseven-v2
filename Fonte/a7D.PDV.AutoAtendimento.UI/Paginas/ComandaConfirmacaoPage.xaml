<Page x:Class="a7D.PDV.AutoAtendimento.UI.Paginas.ComandaConfirmacaoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:a7D.PDV.AutoAtendimento.UI.Paginas"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Loaded="Page_Loaded">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\Controles\StyleButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <Canvas>
        <TextBlock Name="Numero" />
        <TextBlock Name="Nome" />
        <Button Name="Sim" Click="Sim_Click" />
        <Button Name="Nao" Click="Nao_Click" />
        <Button Name="Voltar" Click="Voltar_Click" />
        <Button Name="Home" Click="Home_Click"/>
    </Canvas>
</Page>